0B56:  CLRF   4B
0B58:  MOVLW  03
0B5A:  MOVWF  4C
0B5C:  RCALL  0908
....................             else { 
0B5E:  BRA    0B7C
....................                usb_flush_out(0,USB_DTS_TOGGLE); 
0B60:  CLRF   4B
0B62:  MOVLW  02
0B64:  MOVWF  4C
0B66:  RCALL  0908
....................                if (__setup_0_tx_size!=0xFE) { 
0B68:  MOVF   20,W
0B6A:  SUBLW  FE
0B6C:  BZ    0B7C
....................                   usb_flush_in(0,__setup_0_tx_size,USB_DTS_USERX); 
0B6E:  CLRF   4B
0B70:  CLRF   4D
0B72:  MOVFF  20,4C
0B76:  MOVLW  04
0B78:  MOVWF  4E
0B7A:  RCALL  09EE
....................                } 
....................             } 
....................             UCON_PKTDIS=0;       // UCON,PKT_DIS ; Assuming there is nothing to dequeue, clear the packet disable bit 
0B7C:  BCF    F6D.4
